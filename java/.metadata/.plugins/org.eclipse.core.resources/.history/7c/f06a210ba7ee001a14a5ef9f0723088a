package projetoAula;

public class aula {

	public static void main(String[] args) {
		String resultado;
		int idade = 12;
		
		if (idade < 12) {
			resultado = "Criança";
		} else if (idade >= 12 && idade < 18) {
			resultado = "Adolescente";
		} else {
			resultado = "Adulto";
		}
		
		System.out.println( resultado );

	}

}